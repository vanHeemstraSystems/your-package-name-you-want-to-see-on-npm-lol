{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "commit": "git-cz",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release --branches main"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "your-package-name-you-want-to-see-on-npm-lol",
  "author": "Willem van Heemstra",
  "module": "dist/your-package-name-you-want-to-see-on-npm-lol.esm.js",
  "size-limit": [
    {
      "path": "dist/your-package-name-you-want-to-see-on-npm-lol.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/your-package-name-you-want-to-see-on-npm-lol.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "semantic-release": "^20.1.0",
    "semantic-release-cli": "^5.4.4",
    "size-limit": "^8.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanHeemstraSystems/your-package-name-you-want-to-see-on-npm-lol.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cosmiconfig": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
